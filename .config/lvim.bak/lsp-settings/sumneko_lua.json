{
  "Lua.runtime.version": "LuaJIT",
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": [
    "drvim",
    "lvim",
    "vim",
    "describe",
    "it",
    "before_each",
    "after_each"
  ],
  "Lua.diagnostics.disable": [
    "accessing-undefined-variable",
    "undefined-global",
    "unused-local",
    "unused-vararg",
    "lowercase-global",
    "undefined-field"
  ],
  "Lua.completion.callSnippet": "Both",
  "Lua.completion.keywordSnippet": "Both"
}
