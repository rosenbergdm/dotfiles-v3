{
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": [
    "lvim",
    "vim",
    "describe",
    "it",
    "before_each",
    "after_each"
  ],
  "Lua.diagnostics.disable": [
    "unused-local",
    "unused-vararg",
    "undefined-field"
  ],
  "Lua.completion.callSnippet": "Both",
  "Lua.completion.keywordSnippet": "Both",
  "Lua.hint.enable": true,
  "Lua.hint.setType": true,
  "Lua.workspace.maxPreload": 5000,
  "Lua.workspace.preloadFileSize": 50000,
  "Lua.workspace.library": "/Users/dmr/.local/share/lunarvim/lvim/lua",
  "Lua.IntelliSense": {
    "traceFieldInject": true,
    "traceBeSetted": true,
    "traceReturn": true,
    "autoRequire": true,
    "callSnippet": true,
  }
}
